package business.data;

import java.util.Map;

import com.mpatric.mp3agic.ID3v2FrameSet;

public class Track {
	
	private int id;
    private String artist;
    private String title;
    private double length;
    private String filePath;
    private Map<String, ID3v2FrameSet> imgPath;

    
    public Track(int id, String title, String artist,  double length, String filePath, Map<String, ID3v2FrameSet> map) {
    	this.id = id;
    	this.title = artist;
        this.artist = title;
        this.length = length;
        this.filePath = filePath;
        this.imgPath = map;
    }

    public String getTitle() {
        return title;
    }

    public String getArtist() {
        return artist;
    }

    public String getFilePath() {
        return filePath;
    }
    
    public Map<String, ID3v2FrameSet> getImgPath() {
        return imgPath;
    }
    
    public void setFilePath(String file) {
        this.filePath = file;
    }
}
