public class Track {
	
	private int id;
    private String title;
    private String artist;
    private String filePath;

    
    public Track(int id, String title, String artist, String filePath, int length) {
    	this.id = id;
    	this.title = title;
        this.artist = artist;
        this.filePath = filePath;
    }

    public String getTitle() {
        return title;
    }

    public String getArtist() {
        return artist;
    }

    public String getFilePath() {
        return filePath;
    }
    
    public void setFilePath(String file) {
        this.filePath = file;
    }
}
